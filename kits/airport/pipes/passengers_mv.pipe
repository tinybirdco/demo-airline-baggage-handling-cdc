NODE passengers_mv_0
SQL >

    SELECT
        id,
        updated_at,
        created_at,
        flight_id,
        name,
        status
    FROM passengers_raw

TYPE materialized
DATASOURCE passengers_latest
ENGINE "ReplacingMergeTree"
ENGINE_PARTITION_KEY "toYYYYMM(updated_at)"
ENGINE_SORTING_KEY "id"
ENGINE_VER "updated_at"

